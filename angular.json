{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "deep-travel": {
      "root": "apps/deep-travel/",
      "sourceRoot": "apps/deep-travel/src",
      "projectType": "application",
      "prefix": "dpt",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/deep-travel",
            "index": "apps/deep-travel/src/index.html",
            "main": "apps/deep-travel/src/main.ts",
            "polyfills": "apps/deep-travel/src/polyfills.ts",
            "tsConfig": "apps/deep-travel/tsconfig.app.json",
            "assets": [
              "apps/deep-travel/src/favicon.ico",
              "apps/deep-travel/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/deep-travel/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/deep-travel/src/environments/environment.ts",
                  "with": "apps/deep-travel/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "deep-travel:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "deep-travel:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "deep-travel:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/deep-travel/tsconfig.app.json",
              "apps/deep-travel/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/deep-travel/jest.config.js",
            "tsConfig": "apps/deep-travel/tsconfig.spec.json",
            "setupFile": "apps/deep-travel/src/test-setup.ts"
          }
        }
      }
    },
