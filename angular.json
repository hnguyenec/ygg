{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "deep-travel": {
      "root": "apps/deep-travel/",
      "sourceRoot": "apps/deep-travel/src",
      "projectType": "application",
      "prefix": "dpt",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/deep-travel",
            "index": "apps/deep-travel/src/index.html",
            "main": "apps/deep-travel/src/main.ts",
            "polyfills": "apps/deep-travel/src/polyfills.ts",
            "tsConfig": "apps/deep-travel/tsconfig.app.json",
            "assets": [
              "apps/deep-travel/src/favicon.ico",
              "apps/deep-travel/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/deep-travel/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/deep-travel/src/environments/environment.ts",
                  "with": "apps/deep-travel/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "deep-travel:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "deep-travel:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "deep-travel:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/deep-travel/tsconfig.app.json",
              "apps/deep-travel/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/deep-travel/jest.config.js",
            "tsConfig": "apps/deep-travel/tsconfig.spec.json",
            "setupFile": "apps/deep-travel/src/test-setup.ts"
          }
        }
      }
    },
    "deep-travel-e2e": {
      "root": "apps/deep-travel-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/deep-travel-e2e/cypress.json",
            "tsConfig": "apps/deep-travel-e2e/tsconfig.e2e.json",
            "devServerTarget": "deep-travel:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "deep-travel:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/deep-travel-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "shopping": {
      "root": "libs/shopping",
      "sourceRoot": "libs/shopping/src",
      "projectType": "library",
      "prefix": "ygg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/shopping/tsconfig.lib.json",
            "project": "libs/shopping/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shopping/tsconfig.lib.json",
              "libs/shopping/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/shopping/jest.config.js",
            "tsConfig": "libs/shopping/tsconfig.spec.json",
            "setupFile": "libs/shopping/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "interfaces": {
      "root": "libs/interfaces",
      "sourceRoot": "libs/interfaces/src",
      "projectType": "library",
      "prefix": "ygg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/interfaces/tsconfig.lib.json",
            "project": "libs/interfaces/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/interfaces/tsconfig.lib.json",
              "libs/interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/interfaces/jest.config.js",
            "tsConfig": "libs/interfaces/tsconfig.spec.json"
          }
        }
      },
      "schematics": {}
    },
    "user": {
      "root": "libs/user",
      "sourceRoot": "libs/user/src",
      "projectType": "library",
      "prefix": "ygg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/user/tsconfig.lib.json",
            "project": "libs/user/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/user/tsconfig.lib.json",
              "libs/user/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/user/jest.config.js",
            "tsConfig": "libs/user/tsconfig.spec.json",
            "setupFile": "libs/user/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    },
    "payment": {
      "root": "libs/payment",
      "sourceRoot": "libs/payment/src",
      "projectType": "library",
      "prefix": "ygg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/payment/tsconfig.lib.json",
            "project": "libs/payment/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/payment/tsconfig.lib.json",
              "libs/payment/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/payment/jest.config.js",
            "tsConfig": "libs/payment/tsconfig.spec.json",
            "setupFile": "libs/payment/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "schematics": {},
  "defaultProject": "deep-travel"
}
